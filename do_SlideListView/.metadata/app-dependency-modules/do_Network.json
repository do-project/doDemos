{
  "ID": "do_Network",
  "IsContainer": "0",
  "Type": "SM",
  "Name": "网络状态类",
  "Version": "Internal",
  "Description": "能获取当前手机端的网络状态,监听网络状态的变化",
  "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/R/20150723/464173f6-641e-4383-952a-ff1f818024cb.png",
  "IOS": {
    "Support": 1,
    "MinVersion": "iOS 7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "14"
  },
  "Windows": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "WindowsPhone": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [],
  "Events": [
    {
      "ID": "changed",
      "Description": "网络状态发生变化的回调方法",
      "ReturnType": "String",
      "ReturnDesc": "返回以下枚举类型的一种：'wifi'-wifi网络，'2G/3G/4G'-2G、3G或者4G，'none'-没有网络连接，'unknown'-未知网络连接"
    }
  ],
  "syncMethodes": [
    {
      "ID": "getIP",
      "Name": "获取移动终端ip地址",
      "Description": "获取当前的ip地址",
      "ReturnType": "String",
      "ReturnDesc": "当前的ip地址",
      "Paras": []
    },
    {
      "ID": "getMACAddress",
      "Name": "获取物理地址",
      "Description": "当前设备的物理地址",
      "ReturnType": "String",
      "ReturnDesc": "返回当前设备的物理地址，windows平台返回的地址与直接在手机上查看的不一致，但也表示的是唯一地址",
      "Paras": []
    },
    {
      "ID": "getStatus",
      "Name": "获取当前网络状态",
      "Description": "获取手机端当前处于的网络环境",
      "ReturnType": "String",
      "ReturnDesc": "以下枚举类型的一种：'wifi'-wifi网络，'2G/3G/4G'-2G、3G或者4G，'none'-没有网络连接，'unknown'-未知网络连接",
      "Paras": []
    }
  ],
  "asyncMethodes": []
}