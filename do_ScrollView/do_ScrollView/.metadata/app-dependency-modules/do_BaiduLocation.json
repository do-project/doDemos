{
  "ID": "do_BaiduLocation",
  "IsContainer": "0",
  "Type": "SM",
  "Name": "定位服务",
  "Version": "Internal",
  "Description": "利用设备当前的GPS信息（GPS定位），基站信息（基站定位）和Wi-Fi信息（Wi-Fi定位）完成定位，根据设备当前的实际情况（如是否开启GPS，是否连接网络，是否扫描到Wi-Fi信息等）进行定位",
  "Icon": "https://deviceone.blob.core.chinacloudapi.cn/store/R/20150624/3d1b792b-1d5a-4be8-b756-3d12f85519ad.png",
  "IOS": {
    "Support": 1,
    "MinVersion": "7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 0,
    "MinVersion": ""
  },
  "WindowsPhone": {
    "Support": 0,
    "MinVersion": ""
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [],
  "Events": [
    {
      "ID": "result",
      "Description": "接收到定位/更新定位时候触发",
      "ReturnType": "Node",
      "ReturnDesc": "{latitude:'纬度​'，longitude:'经度', address:'北京市海淀区'}"
    }
  ],
  "syncMethodes": [
    {
      "ID": "start",
      "Name": "开启定位",
      "Description": "开启定位",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "model",
          "Name": "定位模式",
          "Type": "String",
          "Required": 0,
          "DefaultValue": "high",
          "Description": "high:高精度定位模式；low：低精度；middle: 中精度"
        },
        {
          "ID": "isLoop",
          "Name": "是否循环不停的获取",
          "Type": "Bool",
          "Required": 0,
          "DefaultValue": "false",
          "Description": "为false时只获取一次位置，触发一次result事件。 当值为true时,程序会不断获取新的位置（android是根据时间间隔，iOS是根据距离间隔），触发result事件"
        }
      ]
    },
    {
      "ID": "stop",
      "Name": "停止定位",
      "Description": "停止定位",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": []
    }
  ],
  "asyncMethodes": []
}