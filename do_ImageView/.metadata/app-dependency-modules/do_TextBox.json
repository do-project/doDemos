  "IOS": {
    "Support": 1,
    "MinVersion": "iOS 7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "WindowsPhone": {
    "Support": 1,
    "MinVersion": "8.1"
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [
    {
      "ID": "enabled",
      "Name": "是否可编辑",
      "Type": "Bool",
      "DefaultValue": "true",
      "EditType": "1",
      "Group": "",
      "Description": "控制文本框是否为可编辑状态，为false时不可编辑",
      "Design": {
        "Type": "Boolean",
        "Data": []
      }
    },
    {
      "ID": "fontColor",
      "Name": "字体颜色",
      "Type": "String",
      "DefaultValue": "000000FF",
      "EditType": "1",
      "Group": "",
      "Description": "设置字体显示颜色，值格式为：8位16进制字符，前6位是RGB颜色值，后两位是透明度（Alpha），例如：000000FF",
      "Design": {
        "Type": "Color",
        "Data": []
      }
    },
    {
      "ID": "fontSize",
      "Name": "字体大小",
      "Type": "Number",
      "DefaultValue": "17",
      "EditType": "1",
      "Group": "",
      "Description": "",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    },
    {
      "ID": "fontStyle",
      "Name": "字体风格",
      "Type": "String",
      "DefaultValue": "normal",
      "EditType": "1",
      "Group": "",
      "Description": "包含4种类型：normal：常规；bold：粗体；italic：斜体；bold_italic：粗斜体",
      "Design": {
        "Type": "List",
        "Data": [
          {
            "id": "normal",
            "value": "常规"
          },
          {
            "id": "bold",
            "value": "粗体"
          },
          {
            "id": "italic",
            "value": "斜体"
          },
          {
            "id": "bold_italic",
            "value": "粗斜体"
          }
        ]
      }
    },
    {
      "ID": "hint",
      "Name": "text为空提示文本",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "maxLength",
      "Name": "可允许输入的最大长度",
      "Type": "Number",
      "DefaultValue": "100",
      "EditType": "0",
      "Group": "",
      "Description": "",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    },
    {
      "ID": "text",
      "Name": "文本内容",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "获取或设置与此控件关联的文本",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "textFlag",
      "Name": "字体标示",
      "Type": "String",
      "DefaultValue": "normal",
      "EditType": "0",
      "Group": "",
      "Description": "包含3种类型：normal：常规；underline：下划线；strikethrough ：删除线",
      "Design": {
        "Type": "List",
        "Data": [
          {
            "id": "normal",
            "value": "常规"
          },
          {
            "id": "underline",
            "value": "下划线"
          },
          {
            "id": "strikethrough",
            "value": "删除线"
          }
        ]
      }
    }
  ],
  "Events": [
    {
      "ID": "focusIn",
      "Description": "进入编辑状态",
      "ReturnType": "String",
      "ReturnDesc": ""
    },
    {
      "ID": "focusOut",
      "Description": "离开编辑状态",
      "ReturnType": "String",
      "ReturnDesc": ""
    },
    {
      "ID": "textChanged",
      "Description": "文字变化时触发",
      "ReturnType": "String",
      "ReturnDesc": ""
    }
  ],
  "syncMethodes": [
    {
      "ID": "setFocus",
      "Name": "设置焦点",
      "Description": "设置是否得到焦点，得到焦点时软键盘弹出，失去焦点时软键盘收起",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "value",
          "Name": "焦点值",
          "Type": "Bool",
          "Required": 1,
          "DefaultValue": "false",
          "Description": "为true时有焦点，false时无焦点"
        }
      ]
    }
  ],
  "asyncMethodes": []
}